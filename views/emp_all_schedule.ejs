                    customerName: '<%= appointment.User ? (appointment.User.first_name + " " + appointment.User.last_name) : "-" %>',
                    customerNickname: '<%= appointment.User && appointment.User.nickname ? appointment.User.nickname : "-" %>',
                    customerPhone: '<%= appointment.User && appointment.User.phone ? appointment.User.phone : "-" %>',
                    customerEmail: '<%= appointment.User && appointment.User.email ? appointment.User.email : "-" %>',
                    serviceName: '<%= appointment.Service ? appointment.Service.name : "-" %>',
                    servicePrice: '<%= appointment.Service ? parseFloat(appointment.Service.price).toLocaleString("th-TH", { minimumFractionDigits: 2 }) : "0.00" %>',
                    status: '<%= appointment.showstatus %>',
                    employeeIncome: '<%= appointment.employeeIncome > 0 ? appointment.employeeIncome.toLocaleString("th-TH", { minimumFractionDigits: 2 }) : "0.00" %>'
                }<%= index < appointments.length - 1 ? ',' : '' %>
            <% }); %>
        ];

        function openPopup(index) {
            const appointment = appointmentsData[index];
            
            // Populate customer info
            document.getElementById('customerName').textContent = appointment.customerName;
            document.getElementById('customerNickname').textContent = appointment.customerNickname;
            document.getElementById('customerPhone').textContent = appointment.customerPhone;
            document.getElementById('customerEmail').textContent = appointment.customerEmail;
            
            // Populate service details
            document.getElementById('serviceName').value = appointment.serviceName;
            document.getElementById('servicePrice').value = appointment.servicePrice;
            document.getElementById('serviceStatus').value = appointment.status;
            document.getElementById('employeeIncome').value = appointment.employeeIncome;
            
            document.getElementById("popup").classList.remove("hidden");
            document.getElementById("popup").classList.add("flex");
        }
        
        function closePopup() {
            document.getElementById("popup").classList.add("hidden");
            document.getElementById("popup").classList.remove("flex");
        }

        // Filter functionality
        function filterSchedule() {
            const selectedStatus = document.getElementById("status-filter").value;
            const selectedMonth = document.getElementById("month-filter").value;
            const selectedDay = document.getElementById("day-filter").value;
            const appointments = document.querySelectorAll(".appointment-card");

            let visibleCount = 0;

            appointments.forEach(appointment => {
                const appointmentStatus = appointment.dataset.status;
                const appointmentMonth = appointment.dataset.month;
                const appointmentDay = appointment.dataset.day;

                const matchStatus = selectedStatus === "all" || appointmentStatus === selectedStatus;
                const matchMonth = selectedMonth === "all" || appointmentMonth === selectedMonth;
                const matchDay = selectedDay === "all" || appointmentDay === selectedDay;

                if (matchStatus && matchMonth && matchDay) {
                    appointment.style.display = "block";
                    visibleCount++;
                } else {
                    appointment.style.display = "none";
                }
            });

            // Show message if no results
            const noResults = document.getElementById("no-results");
            if (visibleCount === 0) {
                if (!noResults) {
                    const msg = document.createElement('div');
                    msg.id = "no-results";
                    msg.className = "bg-white shadow-sm rounded-lg p-8 border border-gray-200 text-center";
                    msg.innerHTML = '<p class="text-gray-500 text-lg">‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏ó‡∏µ‡πà‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ö‡πÄ‡∏á‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏Ç‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤</p>';
                    document.querySelector(".space-y-3").appendChild(msg);
                }
            } else {
                if (noResults) {
                    noResults.remove();
                }
            }
        }

        function resetFilter() {
            document.getElementById("status-filter").value = "all";
            document.getElementById("month-filter").value = "all";
            document.getElementById("day-filter").value = "all";
            
            const appointments = document.querySelectorAll(".appointment-card");
            appointments.forEach(appointment => {
                appointment.style.display = "block";
            });

            const noResults = document.getElementById("no-results");
            if (noResults) {
                noResults.remove();
            }
        }
    </script>

    <%- include('emp_profile_popup') %>

</body>
</html>
